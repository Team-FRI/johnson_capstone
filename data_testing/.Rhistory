#First do land temp
Painter_LT$YearMonth <- format(as.Date(Painter_LT$DateTime), "%Y-%m")
summary(Painter_LT)
#Now do stream temp
Painter_ST$YearMonth <- format(as.Date(Painter_ST$DateTime), "%Y-%m")
summary(Painter_ST)
